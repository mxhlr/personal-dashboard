{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "NODE_OPTIONS='--max-http-header-size=32768' next dev --turbopack",
    "dev:backend": "convex dev --once",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint .",
    "analyze": "ANALYZE=true npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "perf:test": "lhci autorun",
    "perf:collect": "lhci collect",
    "perf:assert": "lhci assert",
    "perf:upload": "lhci upload",
    "perf:report": "lhci open",
    "perf:parse": "node scripts/parse-lighthouse-report.js",
    "perf:ci": "lhci autorun --config=lighthouserc.js",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:ui": "playwright test --ui",
    "test:visual:debug": "playwright test --debug",
    "test:visual:report": "playwright show-report visual-regression/test-results/html-report",
    "test:visual:chromium": "playwright test --project=chromium",
    "test:visual:firefox": "playwright test --project=firefox",
    "test:visual:webkit": "playwright test --project=webkit",
    "test:visual:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:ci": "playwright test --project=chromium",
    "test:integration": "playwright test tests/integration",
    "test:integration:headed": "playwright test tests/integration --headed",
    "test:integration:ui": "playwright test tests/integration --ui",
    "test:codegen": "playwright codegen http://localhost:3000"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.25.0",
    "@clerk/nextjs": "^6.12.6",
    "@clerk/themes": "^2.4.47",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.71.1",
    "react-masonry-css": "^1.0.16",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
